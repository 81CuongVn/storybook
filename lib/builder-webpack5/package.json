  "devDependencies": {
    "@digitak/esrun": "^3.2.2",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-dev-middleware": "^5.3.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-virtual-modules": "^0.1.1",
    "typescript": "~4.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/custom-webpack-preset.ts",
      "./src/preview-preset.ts"
