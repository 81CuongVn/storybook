      "types": "./dist/preview-preset.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "templates/**/*",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "prepare": "esrun ../../scripts/prepare/bundle.ts"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addons": "7.0.0-alpha.11",
