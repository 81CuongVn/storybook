{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "node",
      "jest"
    ],
    "strict": false,
    "skipLibCheck": false,
    "strictNullChecks": false,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/template*",
    "**/*.test.*",
    "src/frameworks/**/*"
  ]
}