{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "types": [
      "webpack-env"
    ],
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/*.test.*",
    "src/**/tests/**/*",
    "src/**/__tests__/**/*",
    "src/**/*.stories.*",
    "src/**/*.mockdata.*",
    "src/**/__testfixtures__/**"
  ]
}
