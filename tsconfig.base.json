{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": false,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "skipLibCheck": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "target": "ES2015",
    "types": [
      "jest"
    ],
    "lib": [
      "es2019",
      "dom",
      "ESNext"
    ],
    "baseUrl": ".",
    "paths": {
      "@storybook/addon-a11y": ["addons/a11y"],
      "@storybook/addon-actions": ["addons/actions"],
      "@storybook/addon-backgrounds": ["addons/backgrounds"],
      "@storybook/addon-controls": ["addons/controls"],
      "@storybook/addon-docs": ["addons/docs"],
      "@storybook/addon-essentials": ["addons/essentials"],
      "@storybook/addon-interactions": ["addons/interactions"],
      "@storybook/addon-jest": ["addons/jest"],
      "@storybook/addon-links": ["addons/links"],
      "@storybook/addon-measure": ["addons/measure"],
      "@storybook/addon-outline": ["addons/outline"],
      "@storybook/addon-storyshots": ["addons/storyshots/storyshots-core"],
      "@storybook/addon-storyshots-puppeteer": ["addons/storyshots/storyshots-puppeteer"],
      "@storybook/addon-storysource": ["addons/storysource"],
      "@storybook/addon-toolbars": ["addons/toolbars"],
      "@storybook/addon-viewport": ["addons/viewport"],
      "@storybook/angular": ["app/angular"],
      "@storybook/docs-tools": ["lib/docs-tools"],
      "@storybook/ember": ["app/ember"],
      "@storybook/html": ["app/html"],
      "@storybook/manager-webpack4": ["lib/manager-webpack4"],
      "@storybook/manager-webpack5": ["lib/manager-webpack5"],
      "@storybook/preact": ["app/preact"],
      "@storybook/react": ["app/react"],
      "@storybook/server": ["app/server"],
      "@storybook/svelte": ["app/svelte"],
      "@storybook/vue": ["app/vue"],
      "@storybook/vue3": ["app/vue3"],
      "@storybook/web-components": ["app/web-components"],
      "@storybook/addons": ["lib/addons"],
      "@storybook/api": ["lib/api"],
      "@storybook/builder-webpack4": ["lib/builder-webpack4"],
      "@storybook/builder-webpack5": ["lib/builder-webpack5"],
      "@storybook/channel-postmessage": ["lib/channel-postmessage"],
      "@storybook/channel-websocket": ["lib/channel-websocket"],
      "@storybook/channels": ["lib/channels"],
      "@storybook/cli": ["lib/cli"],
      "sb": ["lib/cli-sb"],
      "storybook": ["lib/cli-storybook"],
      "@storybook/client-api": ["lib/client-api"],
      "@storybook/client-logger": ["lib/client-logger"],
      "@storybook/codemod": ["lib/codemod"],
      "@storybook/components": ["lib/components"],
      "@storybook/core": ["lib/core"],
      "@storybook/core-client": ["lib/core-client"],
      "@storybook/core-common": ["lib/core-common"],
      "@storybook/core-events": ["lib/core-events"],
      "@storybook/core-server": ["lib/core-server"],
      "@storybook/instrumenter": ["lib/instrumenter"],
      "@storybook/node-logger": ["lib/node-logger"],
      "@storybook/postinstall": ["lib/postinstall"],
      "@storybook/router": ["lib/router"],
      "@storybook/source-loader": ["lib/source-loader"],
      "@storybook/store": ["lib/store"],
      "@storybook/telemetry": ["lib/telemetry"],
      "@storybook/theming": ["lib/theming"],
      "@storybook/csf-tools": ["lib/csf-tools"],
      "@storybook/ui": ["lib/ui"],
      "@storybook/preview-web": ["lib/preview-web"]
    }
  },
  "exclude": [
    "**/dist",
    "node_modules",
    "**/node_modules",
    "**/*.spec.ts",
    "**/__tests__",
    "**/*.test.ts",
    "**/FlowType*",
    "**/setup-jest.ts"
  ],
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {
      "types": [
        "node"
      ]
    }
  }
}
