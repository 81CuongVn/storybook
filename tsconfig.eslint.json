{
  "extends": "./tsconfig.json",
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "dist",
    "lib/**/dll",
    "build",
    "coverage",
    "node_modules",
    "docs/public",
    "storybook-static",
    "built-storybooks",
    "lib/cli/test",
    "lib/manager-webpack4/prebuilt",
    "lib/manager-webpack5/prebuilt",
    "lib/core-server/prebuilt",
    "lib/codemod/src/transforms/__testfixtures__",
